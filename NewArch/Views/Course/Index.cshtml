@model NewArch.Application.ViewModels.CourseVM


<div class="contaier body-content">
    <div class="row course-content">
        @foreach(var course in Model.Courses)
        {
            <div class="course-image-container">
                <div class="course-image" style="background-image:url(@course.ImageUrl)"></div>
            </div>
            
            <div class="coursecourse-image-container">
                <p>@course.Name</p>
                <p>@course.Description</p>
            </div>
            
        }
    </div>
</div>